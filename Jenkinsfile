// Jenkinsfile (Declarative Pipeline)

pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building.. vamos1'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing.. vamos2'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying.... vamos3'
            }
        }
    }
}